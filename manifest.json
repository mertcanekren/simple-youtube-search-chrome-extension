{
  "name" : "YouTube Search",
  "version" : "1.0",
  "manifest_version" : 2,
  "description" : "YouTube arama.",
  "browser_action" : {
  	"default_title" : "YouTube Arama",
    "default_icon" : "icon.png",
    "default_popup" : "window.html"
  },
  "icons" : {
    "16" : "icon-16.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "background" : {
  	 "scripts" : ["extension.js"]
  },
  "permissions": [
 	"tabs"
  ]
}